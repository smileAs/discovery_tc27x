<plugin>

  <!--
    ** brief AUTOSAR CanTrcv6251G_Aurix plugin
    ** project AUTOSAR Standard Core
    **
    ** All rights exclusively reserved for Elektrobit Automotive GmbH,
    ** unless expressly agreed to otherwise.
  -->

 <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="CanTrcv_17_6251G_AurixAS403_ModuleExt"
             name="CanTrcv_17_6251G Module with template generator">

    <module id="CanTrcv_17_6251G_AurixAS403"
            label="%ModuleLabel"
            mandatory="false"
            allowMultiple="true"
            description="%ModuleDesc"
            copyright="(c) 2013 Infineon Technologies"
            swVersionMajor="2"
            swVersionMinor="2"
            swVersionPatch="1"
            swVersionSuffix=""
            specVersionMajor="3"
            specVersionMinor="0"
            specVersionPatch="0"
            specVersionSuffix=""
            relVersionMajor="4"
            relVersionMinor="0"
            relVersionPatch="3"
            relVersionSuffix=""
            categoryType="CanTrcv_17_6251G"
            categoryLayer="MCAL"
            categoryCategory="ECU Firmware"
            categoryComponent="ECUC">
      <cluster name="Can Stack"/>

            <ecuType target="AURIX" derivate="TC275"/>
      		<ecuType target="AURIX" derivate="TC277"/>
 
    </module>
  </extension>

    <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="CanTrcv_17_6251G_AurixAS403_ConfigExt"
             name="CanTrcv_17_6251G_Aurix Configuration">
    <configuration moduleId="CanTrcv_17_6251G_AurixAS403">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/CanTrcv_17_6251G.xdm" type="xdm"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/AURIX/CanTrcv_17_6251G"/>
      </data>

      <editor id="CanTrcv_17_6251G_AurixAS403_Edit"
              label="%ModuleName"
              tooltip="%ModuleDesc">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/AURIX/CanTrcv_17_6251G"/>
          <parameter name="title" value="CanTrcv_17_6251G"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>

   <!-- 
     This extension draws the connection between the module, and the com importer.
   -->   
  <extension point="dreisoft.tresos.comimporter.api.plugin.comtransformer">
    <comtransformer
           id="ComTransformer_CanTrcv_17_6251G_AurixAS403"
           moduleId="CanTrcv_17_6251G_AurixAS403">
      <transformer
             class="dreisoft.tresos.comimporter.api.transformer.asr40.CanTrcvTransformer">
      </transformer>
    </comtransformer>
  </extension>   
  <extension point="dreisoft.tresos.launcher2.plugin.generator"
              id="CanTrcv_17_6251G_AurixAS403_GenExt"
              name="CanTrcv_17_6251G_Aurix Generator">
    <generator moduleId="CanTrcv_17_6251G_AurixAS403"
               class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator">

      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate"/>

      <!-- swcd modes and template path parameters -->
      <parameter name="mode_type"
                 mode="generate_swcd" value="generate"/>

      <parameter name="mode_type"
                 mode="verify_swcd" value="verify"/>

      <parameter name="templates"
                 mode="generate_swcd,verify_swcd" value="generate_swcd"/>

    </generator>
  </extension>

</plugin>

