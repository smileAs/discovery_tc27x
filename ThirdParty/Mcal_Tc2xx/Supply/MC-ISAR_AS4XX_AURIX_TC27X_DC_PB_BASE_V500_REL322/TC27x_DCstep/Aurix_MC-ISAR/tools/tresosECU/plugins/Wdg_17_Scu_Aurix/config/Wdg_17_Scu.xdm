  <d:ctr type="AUTOSAR" factory="autosar"
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd"
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd"
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
    <d:lst type="TOP-LEVEL-PACKAGES">
      <d:ctr name="AURIX" type="AR-PACKAGE">
        <a:a name="UUID" value="05ea1b12-0cdd-4366-8b56-45684d0f2cc7"/>
        <d:lst type="ELEMENTS">
          <d:chc name="Wdg" type="AR-ELEMENT" value="MODULE-DEF">
            <v:ctr type="MODULE-DEF">
              <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                <ad:ADMIN-DATA>
                  <ad:DOC-REVISIONS>
                    <ad:DOC-REVISION>
                      <ad:REVISION-LABEL>4.2.0</ad:REVISION-LABEL>
                      <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                      <ad:DATE>2011-11-09</ad:DATE>
                    </ad:DOC-REVISION>
                  </ad:DOC-REVISIONS>
                </ad:ADMIN-DATA>
              </a:a>
              <a:a name="RELEASE" value="asc:4.0.3"/>
              <a:a name="UUID" value="29fb3605-abf0-409f-b333-4efedfd1fb68"/>
              <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                     type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                 </a:a>
                <a:a name="LABEL" value="Config Variant"/>
                <a:da name="DEFAULT" value="VariantPostBuildSelectable"/>
                <a:da name="EDITABLE" value="true"/>
                <a:da name="RANGE">
                   <a:v>VariantPostBuildLoadable</a:v>
                   <a:v>VariantPostBuildSelectable</a:v>
                 </a:da>
              </v:var>
              <d:ref type="REFINED_MODULE_DEF" value="ASPath:/AUTOSAR/EcucDefs/Wdg"/>
              <v:ctr name="WdgGeneral" type="IDENTIFIABLE">
                <a:a name="DESC"
                     value="EN: All general parameters of the watchdog driver are collected here."/>
                <a:a name="UUID" value="a9636539-a01b-4aef-a049-ce50f4b2813d"/>
                <v:var name="WdgDevErrorDetect" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN: Compile switch to enable / disable development error detection for this module.

True: Development error detection enabled
False: Development error detection disabled</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="945af739-40d0-4d71-8adc-250e053100dd"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="WdgDisableAllowed" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN: Compile switch to allow / forbid disabling the watchdog driver during runtime.

True: Disabling the watchdog driver at runtime is allowed.
False: Disabling the watchdog driver at runtime is not allowed.</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="c2ec6a16-c903-4125-811e-df164accf984"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="WdgIndex" type="INTEGER">
                  <a:a name="DESC"
                       value="EN: Represents the watchdog driver&apos;s ID so that it can be referenced by the watchdog interface."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                  <a:a name="UUID" value="db48ae8e-48bc-408f-b2b0-20859a6d5d33"/>
                  <a:da name="DEFAULT" value="0"/>
                  <a:da name="INVALID" type="Range">
                    <a:tst expr="&lt;=255"/>
                    <a:tst expr="&gt;=0"/>
                  </a:da>
                </v:var>
                <v:var name="WdgTriggerLocation" type="FUNCTION-NAME">
                  <a:a name="DESC"
                       value="EN: Location (memory address) of the watchdog trigger routine."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="1e9b9363-6628-470d-ab93-116e19381518"/>
                  <a:da name="EDITABLE" value="false"/>
                </v:var>
                <v:var name="WdgVersionInfoApi" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN: Compile switch to enable / disable the version information API
                          True: API enabled
                          False: API disables</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="26affa9b-f445-4719-bb13-421950e150f7"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="WdgPBFixedAddress" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN: This parameter indicates whether the Wdg configuration to be used is decided at link time or changeable by user at runtime.
           ON : WDG Configuration to be used is fixed i.e Wdg_ConfigRoot[0]
           OFF: The user has to choose the WDG configuration to be used and pass the same as a parameter to Wdg_Init..</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="26affa9b-f445-4719-bb13-421950e150f8"/>
                  <a:da name="DEFAULT" value="false"/>
                  <a:da name="INVALID" type="XPath">
                  <a:tst expr="(num:i(count(../../WdgSettingsConfig/*))) > 1 and . = 'true'"
                   true="Wrong Configuration. When WdgPBFixedAddress is enabled then only 1 Config set is permitted. Remove the other Config sets."/>
                  </a:da>
                </v:var> 
                 
                 <v:var name="WdgMaxTimeout" type="FLOAT">
                  <a:a name="DESC"
                       value="EN: The maximum timeout (sec) to which the watchdog trigger condition can be initialized."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="db48ae8e-48bc-408f-b2b0-20949a6d5d33"/>
                  <a:da name="DEFAULT" value="32.0"/>
                  <a:da name="INVALID" type="Multi">
                    <mt:xpath expr=". > 65.535" true="WdgMaxTimeout should be less than or equal to 65.535"/>
                    <mt:xpath expr=". &lt; 0.001" true="WdgMaxTimeout should not be less than 0.001"/>
                    <mt:xpath expr=". = 0" true="WdgMaxTimeout cannot be 0"/>
                    <mt:range>
                    <mt:tst expr="&gt;=0.001"/>
                    <mt:tst expr="&lt;=65.535"/>
                    </mt:range>
                  </a:da>
                </v:var>
                    
                <v:var name="WdgInitialTimeout" type="FLOAT">
                  <a:a name="DESC"
                       value="EN: The initial timeout (sec) for the trigger condition to be initialized during Init function. It shall be not larger than WdgMaxTimeout."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="db48ae8e-48bc-408f-b2b0-20856a6d5d33"/>
                  <a:da name="DEFAULT" value="5.0"/>
                  <a:da name="INVALID" type="Multi">
                    
                    <mt:xpath expr=". > ../WdgMaxTimeout" true="WdgInitialTimeout should be less than or equal to WdgMaxTimeout"/>
                    
                    <mt:range>
                    <mt:tst expr="&gt;=0.001"/>
                    <mt:tst expr="&lt;=65.535"/>
                    </mt:range>
                  </a:da>
                </v:var>
                <v:var name="WdgDebugSupport" type="BOOLEAN">
                  <a:a name="DESC"
                       value="EN: Pre-processor switch for enabling/disabling the debugging support."/>
                  <a:a name="TOOLTIP"
                       value="Pre-processor switch for enabling/disabling the debugging support."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="01eee1fb-1589-455e-9b2b-cead4d920436"/>
                  <a:da name="DEFAULT" value="false"/>
                </v:var>
                <v:var name="WdgRunArea" type="ENUMERATION">
                  <a:a name="DESC">
                    <a:v>EN: Represents the watchdog driver execution area which is either from ROM(Flash) or RAM as required with the particular microcontroller.</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID"
                       value="6295e90d-7299-4a17-b80f-a8646663041g"/>
                  <a:da name="DEFAULT" value="ROM"/>
                  <a:da name="EDITABLE" value="false"/>
                  <a:da name="RANGE">
                    <a:v>ROM</a:v>
                    <a:v>RAM</a:v>
                  </a:da>
                </v:var>
           <v:var name="WdgUnlockRestriction" type="BOOLEAN">
                  <a:a name="DESC">
                    <a:v>EN: Request to enable/disable SMU restriction of WDT unlock
                         True: Watchdog driver is enabled with SMU restriction of WDT unlock
                         False: Watchdog driver is disabled with SMU restriction of WDT unlock.</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                    <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                    <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="6295e90d-7299-6a28-b80f-a8646663041g"/>
                  <a:da name="DEFAULT" value="true"/>
                </v:var>
		<v:var name="WdgRunningInUser0Mode" type="BOOLEAN">
                  <a:a name="DESC"
                       value="If this parameter is set to FALSE, then Protected Mode(user mode) register access will not be done and API is run in Supervisor mode."/>
                  <a:a name="TOOLTIP"
                       value="EN:Pre-processor switch to enable/disable the Normal/Protected mode."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="2496a517-5ef8-487e-51e0-0234c58e9bf0"/>
                  <a:da name="DEFAULT" value="false"/>
               </v:var>
               <v:var name="WdgUserModeInitApiEnable" type="BOOLEAN">
                  <a:a name="DESC"
                       value="Pre-processor switch to enable / disable protected register access in all the Init API."/>
                  <a:a name="TOOLTIP"
                       value="EN:Pre-processor switch to enable / disable protected register access in all the Init API."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="2496a587-5ef8-487e-51e0-0234c58e9bf0"/>
                  <a:da name="DEFAULT" value="false"/>
               </v:var>
               <v:var name="WdgUserModeRuntimeApiEnable" type="BOOLEAN">
                  <a:a name="DESC"
                       value="Pre-processor switch to enable / disable protected register access in all the APIs other than Init APIs."/>
                  <a:a name="TOOLTIP"
                       value="EN:Pre-processor switch to enable / disable protected register access in all the APIs other than Init APIs."/>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS"
                       type="IMPLEMENTATIONCONFIGCLASS">
                   <icc:v class="PreCompile">VariantPostBuildSelectable</icc:v>
                   <icc:v class="PreCompile">VariantPostBuildLoadable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                  <a:a name="UUID" value="3496a537-5ef8-487e-51e0-0234c58e9bf0"/>
                  <a:da name="DEFAULT" value="false"/>
               </v:var>
              </v:ctr>
              <v:lst name="WdgSettingsConfig" type="MULTIPLE-CONFIGURATION-CONTAINER">
                <a:da name="MIN" value="1"/>
                <v:ctr name="WdgSettingsConfig" type="MULTIPLE-CONFIGURATION-CONTAINER">
                  <a:a name="DESC"
                       value="EN: Configuration items for the different watchdog modes"/>
                  <a:a name="UUID" value="d567be7b-4f29-4c2a-a3af-2ef6f8f97404"/>
                  <v:ref name="WdgSystemClockRef" type="REFERENCE">
                  <a:a name="DESC">
                   <a:v>FR: This parameter refers to the system clock configured using the Mcu_InitClock function.

This parameter should refer to a valid McuClockSetting Configuration (/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint)

Also the WDG module will work correctly only for this system clock setting. No changes should be done to the system clock if WDG is to be kept functioning.</a:v>
                  </a:a>
                  <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                      type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                  </a:a>
                  <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                  <a:a name="UUID" value="9ef075e8-51b4-4d4b-bb53-fc516d8f2b23"/>
                  <a:da name="REF"
                        value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Mcu/McuModuleConfiguration/McuClockSettingConfig/McuClockReferencePoint"/>
                  </v:ref>
                  <v:var name="WdgDefaultMode" type="ENUMERATION">
                    <a:a name="DESC">
                      <a:v>EN: Default mode for watchdog driver initialization for CPU core 0.

ImplementationType: WdgIf_ModeType</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID"
                         value="6295e90d-7299-4a17-b80f-a8646663041e"/>
                    <a:da name="DEFAULT" value="WDGIF_FAST_MODE"/>
                    <a:da name="RANGE">
                      <a:v>WDGIF_FAST_MODE</a:v>
                      <a:v>WDGIF_OFF_MODE</a:v>
                      <a:v>WDGIF_SLOW_MODE</a:v>
                    </a:da>
                   <a:da name="INVALID" type="XPath">
                   <a:tst expr="(//WdgDisableAllowed = 'false' or //Wdg0SafetyEnable = 'true') and .= 'WDGIF_OFF_MODE'" true="WDGIF_OFF_MODE is only possible if disabling of Watchdog Driver is allowed"/>
                   </a:da>
                  </v:var>
                 <v:var name="Wdg1DefaultMode" type="ENUMERATION">
                    <a:a name="DESC">
                      <a:v>EN: Default mode for watchdog driver initialization for CPU core 1.
ImplementationType: WdgIf_ModeType</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
                      <icc:v class="PostBuild">VariantPostBuildLoadable</icc:v>
                      <icc:v class="PostBuild">VariantPostBuildSelectable</icc:v>
                    </a:a>
                    <a:a name="ORIGIN" value="INFINEON_AURIX"/>
                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                    <a:a name="UUID"
                         value="6295e90d-7299-4a17-b80f-b8646663141e"/>
                    <a:da name="DEFAULT" value="WDGIF_FAST_MODE"/>
                    <a:da name="RANGE">
                      <a:v>WDGIF_FAST_MODE</a:v>
                      <a:v>WDGIF_OFF_MODE</a:v>
                      <a:v>WDGIF_SLOW_MODE</a:v>
                    </a:da>
                   <a:da name="INVALID" type="XPath">
                   <a:tst expr="(//WdgDisableAllowed = 'false' or //Wdg1SafetyEnable = 'true') and .= 'WDGIF_OFF_MODE'" true="WDGIF_OFF_MODE is only possible if disabling of Watchdog1 Driver is allowed"/>
                   </a:da>
                  </v:var>
                 <v:var name="Wdg2DefaultMode" type="ENUMERATION">
                 <a:da name="ENABLE" type="XPath" expr="ecu:get('Wdg.MaxTimers')=3"/>
                    <a:a name="DESC">
                      <a:v>EN: Default mode for watchdog driver initialization for CPU core 2.
ImplementationType: WdgIf_ModeType</a:v>
                    </a:a>
                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                         type="IMPLEMENTATIONCONFIGCLASS">
