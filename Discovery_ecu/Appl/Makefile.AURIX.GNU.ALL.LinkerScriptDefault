define LINK_LOCATE_RULE_TC2XX
  $(ECHO) "  "                                                                                                                        > $@; \
  $(ECHO) " /* "                                                                                                                      >> $@; \
  $(ECHO) "  * Name: iROM.ld  "                                                                                                       >> $@; \
  $(ECHO) "  *  "                                                                                                                     >> $@; \
  $(ECHO) "  * Generated Linker Description File "                                                                                    >> $@; \
  $(ECHO) "  * Copyright (C) 2010 HighTec EDV-Systeme GmbH.  "                                                                        >> $@; \
  $(ECHO) "  * (!Do not edit outsite of the protection areas!) "                                                                      >> $@; \
  $(ECHO) "  * "                                                                                                                      >> $@; \
  $(ECHO) "  * Description:  "                                                                                                        >> $@; \
  $(ECHO) "  * internal flash configuration "                                                                                         >> $@; \
  $(ECHO) "  */ "                                                                                                                     >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; \
  $(ECHO) " /* "                                                                                                                      >> $@; \
  $(ECHO) "  * Define Entrypoint of Executable "                                                                                      >> $@; \
  $(ECHO) "  */ "                                                                                                                     >> $@; \
  $(ECHO) " ENTRY(_start) "                                                                                                           >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; \
  $(ECHO) " /* "                                                                                                                      >> $@; \
  $(ECHO) "  * Global "                                                                                                               >> $@; \
  $(ECHO) "  */ "                                                                                                                     >> $@; \
  $(ECHO) " /*Program Flash Memory (PFLASH0)*/  "                                                                                     >> $@; \
  $(ECHO) " __PMU_PFLASH0_BEGIN = 0xA0000000; "                                                                                       >> $@; \
  $(ECHO) " __PMU_PFLASH0_SIZE = 2M; "                                                                                                >> $@; \
  $(ECHO) " /*Program Flash Memory (PFLASH1)*/  "                                                                                     >> $@; \
  $(ECHO) " __PMU_PFLASH1_BEGIN = 0xA0200000; "                                                                                       >> $@; \
  $(ECHO) " __PMU_PFLASH1_SIZE = 2M; "                                                                                                >> $@; \
  $(ECHO) " /*Data Flash Memory (DFLASH0)*/  "                                                                                        >> $@; \
  $(ECHO) " __PMU_DFLASH0_BEGIN = 0xAF000000; "                                                                                       >> $@; \
  $(ECHO) " __PMU_DFLASH0_SIZE = 1M; "                                                                                                >> $@; \
  $(ECHO) " /*Data Flash Memory (DFLASH0_1)*/  "                                                                                      >> $@; \
  $(ECHO) " __PMU_DFLASH0_1_BEGIN = 0xAF100000; "                                                                                     >> $@; \
  $(ECHO) " __PMU_DFLASH0_1_SIZE = 16K; "                                                                                             >> $@; \
  $(ECHO) " /*Data Flash Memory (DFLASH1)*/  "                                                                                        >> $@; \
  $(ECHO) " __PMU_DFLASH1_BEGIN = 0xAF110000; "                                                                                       >> $@; \
  $(ECHO) " __PMU_DFLASH1_SIZE = 64K; "                                                                                               >> $@; \
  $(ECHO) " /*Boot ROM (BROM)*/  "                                                                                                    >> $@; \
  $(ECHO) " __BROM_BEGIN = 0x8FFF8000; "                                                                                              >> $@; \
  $(ECHO) " __BROM_SIZE = 32K; "                                                                                                      >> $@; \
  $(ECHO) " /*Scratch-Pad RAM (PSPR)*/  "                                                                                             >> $@; \
  $(ECHO) " __PMI_PSPR_BEGIN = 0xC0000000; "                                                                                          >> $@; \
  $(ECHO) " __PMI_PSPR_SIZE = 24K; "                                                                                                  >> $@; \
  $(ECHO) " /*Local Data RAM (DSPR)*/  "                                                                                              >> $@; \
  $(ECHO) " __DMI_DSPR_BEGIN = 0xD0000000; "                                                                                          >> $@; \
  $(ECHO) " __DMI_DSPR_SIZE = 112K; "                                                                                                 >> $@; \
  $(ECHO) " /*Local Data RAM (DSPR)*/  "                                                                                              >> $@; \
  $(ECHO) " __LMU_SRAM_BEGIN = 0x90000000; "                                                                                          >> $@; \
  $(ECHO) " __LMU_SRAM_SIZE = 32K; "                                                                                                  >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; \
  $(ECHO) " __USTACK_SIZE = DEFINED (__USTACK_SIZE) ? __USTACK_SIZE : 1K;  /* Section for ustack*/  "                                 >> $@; \
  $(ECHO) " __ISTACK_SIZE = DEFINED (__ISTACK_SIZE) ? __ISTACK_SIZE : 256;  /* Section for istack*/  "                                >> $@; \
  $(ECHO) " __HEAP_SIZE = DEFINED (__HEAP_SIZE) ? __HEAP_SIZE : 4K;  /* Section for heap*/  "                                         >> $@; \
  $(ECHO) " __CSA_SIZE = DEFINED (__CSA_SIZE) ? __CSA_SIZE : 16K;  /* Section for CSA*/  "                                            >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; \
  $(ECHO) " /** "                                                                                                                     >> $@; \
  $(ECHO) "  * User defined global region "                                                                                           >> $@; \
  $(ECHO) "  */ "                                                                                                                     >> $@; \
  $(ECHO) " /*PROTECTED REGION ID(Protection:iROM-Global) ENABLED START*/ "                                                           >> $@; \
  $(ECHO) " /*Protection-Area for your own LDF-Code*/ "                                                                               >> $@; \
  $(ECHO) " /*PROTECTED REGION END*/ "                                                                                                >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; \
  $(ECHO) " /* "                                                                                                                      >> $@; \
  $(ECHO) "  * internal flash configuration "                                                                                         >> $@; \
  $(ECHO) "  */ "                                                                                                                     >> $@; \
  $(ECHO) " MEMORY "                                                                                                                  >> $@; \
  $(ECHO) " { "                                                                                                                       >> $@; \
  $(ECHO) "  PMU_PFLASH0 (rx!p): org = 0x80080000, len = 1M  /*Program Flash Memory (PFLASH0)*/  "                                    >> $@; \
  $(ECHO) "  PMU_PFLASH1 (rx!p): org = 0x80200000, len = 2M  /*Program Flash Memory (PFLASH1)*/  "                                    >> $@; \
  $(ECHO) "  PMU_DFLASH0 (r!xp): org = 0xAF000000, len = 1M  /*Data Flash Memory (DFLASH0)*/  "                                       >> $@; \
  $(ECHO) "  PMU_DFLASH0_1 (r!xp): org = 0xAF100000, len = 16K  /*Data Flash Memory (DFLASH0_1)*/  "                                  >> $@; \
  $(ECHO) "  PMU_DFLASH1 (r!xp): org = 0xAF110000, len = 64K  /*Data Flash Memory (DFLASH1)*/  "                                      >> $@; \
  $(ECHO) "  BROM (rx!p): org = 0x8FFF8000, len = 32K  /*Boot ROM (BROM)*/  "                                                         >> $@; \
  $(ECHO) "  PMI_PSPR (wx!p): org = 0xC0000000, len = 24K  /*Scratch-Pad RAM (PSPR)*/  "                                              >> $@; \
  $(ECHO) "  DMI_DSPR (w!xp): org = 0xD0000000, len = 112K  /*Local Data RAM (DSPR)*/  "                                              >> $@; \
  $(ECHO) "  LMU_SRAM (w!xp): org = 0x90000000, len = 32K  /*Local Data RAM (DSPR)*/  "                                               >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) " } "                                                                                                                       >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; \
  $(ECHO) " SECTIONS "                                                                                                                >> $@; \
  $(ECHO) " { "                                                                                                                       >> $@; \
  $(ECHO) "  /*Code-Sections*/ "                                                                                                      >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Startup code for AURIX "                                                                                              >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .startup_code  : "                                                                                                       >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__startup_code_start = .); "                                                                                    >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .startup_code.begin) ENABLED START*/ "                                           >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   *(.startup_code) /*Startup code for AURIX*/  "                                                                          >> $@; \
  $(ECHO) "   *(.startup_code*) "                                                                                                     >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .startup_code) ENABLED START*/ "                                                 >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__startup_code_end = .); "                                                                                      >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "  } > PMU_PFLASH0 /* PMU_PFLASH0: Program Flash Memory (PFLASH0) */  "                                                     >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Code section "                                                                                                        >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .text  : "                                                                                                               >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__text_start = .); "                                                                                            >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .text.begin) ENABLED START*/ "                                                   >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   *(.text) /*Code section*/  "                                                                                            >> $@; \
  $(ECHO) "   *(.text*) "                                                                                                             >> $@; \
  $(ECHO) "   *(.gnu.linkonce.t.*) "                                                                                                  >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .text) ENABLED START*/ "                                                         >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__text_end = .); "                                                                                              >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "  } > PMU_PFLASH0 /* PMU_PFLASH0: Program Flash Memory (PFLASH0) */  "                                                     >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Code executed before calling main "                                                                                   >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .init  : "                                                                                                               >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__init_start = .); "                                                                                            >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .init.begin) ENABLED START*/ "                                                   >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   KEEP(*(.init)) /*Code executed before calling main*/  "                                                                 >> $@; \
  $(ECHO) "   KEEP(*(.init*)) "                                                                                                       >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .init) ENABLED START*/ "                                                         >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__init_end = .); "                                                                                              >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "  } > PMU_PFLASH0 /* PMU_PFLASH0: Program Flash Memory (PFLASH0) */  "                                                     >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Code executed before exiting program "                                                                                >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .fini  : "                                                                                                               >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   PROVIDE(__fini_start = .); "                                                                                            >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .fini.begin) ENABLED START*/ "                                                   >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
