  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   KEEP(*(.fini)) /*Code executed before exiting program*/  "                                                              >> $@; \
  $(ECHO) "   KEEP(*(.fini*)) "                                                                                                       >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
  $(ECHO) "   /*PROTECTED REGION ID(Protection: iROM .fini) ENABLED START*/ "                                                         >> $@; \
  $(ECHO) "   /*Protection-Area for your own LDF-Code*/ "                                                                             >> $@; \
  $(ECHO) "   /*PROTECTED REGION END*/ "                                                                                              >> $@; \
  $(ECHO) "    "                                                                                                                      >> $@; \
