  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    __USTACK_BEGIN = . ; "                                                                                                 >> $@; \
  $(ECHO) "   . += __USTACK_SIZE ; "                                                                                                  >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    __USTACK = . ; "                                                                                                       >> $@; \
  $(ECHO) "    __USTACK_END = . ; "                                                                                                   >> $@; \
  $(ECHO) "  } > DMI_DSPR "                                                                                                           >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Section for istack "                                                                                                  >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .istack : "                                                                                                              >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    __ISTACK_BEGIN = . ; "                                                                                                 >> $@; \
  $(ECHO) "   . += __ISTACK_SIZE ; "                                                                                                  >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    __ISTACK = . ; "                                                                                                       >> $@; \
  $(ECHO) "    __ISTACK_END = . ; "                                                                                                   >> $@; \
  $(ECHO) "  } > DMI_DSPR "                                                                                                           >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Section for heap "                                                                                                    >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .heap : "                                                                                                                >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    __HEAP_BEGIN = . ; "                                                                                                   >> $@; \
  $(ECHO) "    __HEAP = . ; "                                                                                                         >> $@; \
  $(ECHO) "   . += __HEAP_SIZE ; "                                                                                                    >> $@; \
  $(ECHO) "   . = ALIGN(8); "                                                                                                         >> $@; \
  $(ECHO) "    __HEAP_END = . ; "                                                                                                     >> $@; \
  $(ECHO) "  } > DMI_DSPR "                                                                                                           >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /* "                                                                                                                     >> $@; \
  $(ECHO) "   * Section for CSA "                                                                                                     >> $@; \
  $(ECHO) "   */ "                                                                                                                    >> $@; \
  $(ECHO) "  .csa : "                                                                                                                 >> $@; \
  $(ECHO) "  { "                                                                                                                      >> $@; \
  $(ECHO) "   . = ALIGN(64); "                                                                                                        >> $@; \
  $(ECHO) "    __CSA_BEGIN = . ; "                                                                                                    >> $@; \
  $(ECHO) "    __CSA = . ; "                                                                                                          >> $@; \
  $(ECHO) "   . += __CSA_SIZE ; "                                                                                                     >> $@; \
  $(ECHO) "   . = ALIGN(64); "                                                                                                        >> $@; \
  $(ECHO) "    __CSA_END = . ; "                                                                                                      >> $@; \
  $(ECHO) "  } > DMI_DSPR "                                                                                                           >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) "  /*PROTECTED REGION ID(Protection:iROM-User-Sections) ENABLED START*/ "                                                   >> $@; \
  $(ECHO) "  /*Protection-Area for your own LDF-Code*/ "                                                                              >> $@; \
  $(ECHO) "  /*PROTECTED REGION END*/ "                                                                                               >> $@; \
  $(ECHO) "   "                                                                                                                       >> $@; \
  $(ECHO) " } "                                                                                                                       >> $@; \
  $(ECHO) "  "                                                                                                                        >> $@; 
endef

